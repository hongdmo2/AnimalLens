# Example environment variables - Copy this file to create .env.development and .env.production

# AWS Configuration
AWS_ACCESS_KEY_ID=your_access_key        # AWS 접근 키 입력
AWS_SECRET_ACCESS_KEY=your_secret_key    # AWS 비밀 키 입력
AWS_REGION=us-east-1                     # 사용할 AWS 리전
S3_BUCKET=your_bucket_name              # S3 버킷 이름

# Database Configuration
DB_USER=postgres                        # 데이터베이스 사용자
DB_PASSWORD=your_password              # 사용자 비밀번호
DB_HOST=postgres                       # 호스트 (Docker 서비스 이름)
DB_PORT=5432
DB_NAME=animallens

# Service URLs
FRONTEND_URL=http://localhost:3000  # Development/your-domain.com Production
BACKEND_URL=http://localhost:8000   # Development/api.your-domain.com Production 